{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "auth0/jwt-auth-bundle": "^3.1",
        "friendsofsymfony/elastica-bundle": "^5.0",
        "friendsofsymfony/user-bundle": "^2.1",
        "guzzlehttp/guzzle": "^6.3",
        "guzzlehttp/psr7": "^1.4",
        "hwi/oauth-bundle": "^0.6.3",
        "jms/serializer-bundle": "^2.4",
        "nelmio/cors-bundle": "^1.5",
        "neomerx/json-api": "^2.0",
        "overblog/graphql-bundle": "^0.11.4",
        "php-http/curl-client": "^1.7",
        "php-http/httplug-bundle": "^1.11",
        "prooph/event-sourcing": "^5.6",
        "prooph/event-store-bus-bridge": "^3.3",
        "prooph/event-store-symfony-bundle": "^0.5.0",
        "prooph/pdo-event-store": "^1.9",
        "prooph/service-bus-symfony-bundle": "^0.7.0",
        "ramsey/uuid-doctrine": "dev-master",
        "symfony/console": "*",
        "symfony/flex": "^1.1",
        "symfony/framework-bundle": "*",
        "symfony/messenger": "*",
        "symfony/orm-pack": "^1.0",
        "symfony/serializer": "*",
        "symfony/yaml": "*",
        "vich/uploader-bundle": "^1.8",
        "zendframework/zend-validator": "^2.10"
    },
    "require-dev": {
        "overblog/graphiql-bundle": "^0.1.1",
        "symfony/dotenv": "*",
        "symfony/maker-bundle": "^1.6"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.1.*"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
